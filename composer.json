{
    "name": "cryptomkt/cryptomkt-curl-php",
    "description": "PHP Client to integrate CryptoMarket into your PHP Projects",
    "type": "git",
    "require": {
        "psr/log": "^1.0",
        "php-http/curl-client": "^1.7",
        "php-http/message": "^1.6",
        "zendframework/zend-diactoros": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": { 
            "Cryptomkt\\": "src/", 
            "Cryptomkt\\Exchange\\": "src/", 
            "Cryptomkt\\Psr7\\": "src/" 
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Pando",
            "email": "mario@cryptomkt.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "provide": {
        "psr/http-message-implementation": "1.0"
    }
}
